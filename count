package swap.java;
import java.util.Scanner;

 class count {
	 public static void main(String args[]) {
		 Scanner sc=new Scanner(System.in);
		 int sum=0;
		 int num;
		 num=sc.nextInt();
		 
		 while(num!=0) {
			 sum+=num%10;
			
			 num=num/10;
			
		 }
		 System.out.println(sum);
		 
		 
		 
	 }

}
